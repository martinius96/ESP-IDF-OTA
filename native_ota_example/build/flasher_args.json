    "bootloader" : { "offset" : "0x1000", "file" : "bootloader/bootloader.bin" },
    "app" : { "offset" : "0x20000", "file" : "native_ota.bin" },
    "extra_esptool_args" : {
        "after"  : "no_reset",
        "before" : "default_reset"
    }
